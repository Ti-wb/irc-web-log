  // performance test
  var totalMedia = 0,
      totalMediaTime = 0,
      totalText = 0,
      totalTextTime = 0,
      mesureStart, mesure;
  
  if (typeof debug !== 'undefined' && debug === true) {
    (function () {
      var time = null;
      mesureStart = function () {
        time = Date.now();
      }
      mesure = function () {
        return Date.now() - time;
      }
    } ())
  }
