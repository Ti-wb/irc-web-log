  <body>
    <div class="anchor" id="top"></div>
    <!--<script src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.10.3/moment-with-locales.min.js"></script>-->
    <% include _header.ejs %>
    <div id="messages">
        <div class="message head">
          <div class="time">time</div>
          <div class="from">from</div>
          <div class="word">message</div>
        </div> 
        <% messages.forEach(function(message) { %>
          <% if (message.medias && message.medias.length > 0) { %>
            <div id="<%= message._id %>" class="message">
              <div class="time"><a href="#<%= message._id %>"><%= 
                moment(message.time)
                .utcOffset(globalConfig.timezone)
                .locale(globalConfig.locale) 
                .format('a hh:mm:ss')
              %></a></div>
              <div class="from">
                <a style="color: <%= getColor(message.from); %>" href="/message/<%= message._id %>">
                  <%= message.from %>
                </a>
              </div>
              <div class="word"><!--
                --><% message.medias.forEach(function (media) { %><!--
                  --><% media.files.forEach(function (file) { %><!--
                    --><% if (media.role !== 'audio') { %><!--
                      --><% if (!file.isThumb) {return} %><!--
                      --><img class="lazy media media_<%= media.role %>" data-original="/files/<%= file.contentSrc %>" 
                          <% if (file.photoSize && file.photoSize.length === 2 ) { %>
                          width=<%= file.photoSize[0] %> height=<%= file.photoSize[1] %>
                          <% } %> 
                          data-role="<%= media.role %>"
                          data-media-id="<%= media._id %>"
                          data-mime="<%= file.MIME %>"
                          alt="[<%= media.role %>](media:<%= file.contentSrc %>)"><!--
                    --><% } else { %><!--
                      --><audio controls preload="none">
                          <source src="/files/<%= file.contentSrc %>" type="<%= file.MIME %>">
                        </audio><!--
                    --><% } %><!--
                  --><% }) %><!--   
                --><% }) %><!--
              --></div>
            </div>
          <% } else if (!message.message.match(/^\u0001ACTION.+\u0001$/i)) { %>
            <div id="<%= message._id %>" class="message">
              <div class="time"><a href="#<%= message._id %>"><%= 
                moment(message.time)
                .utcOffset(globalConfig.timezone)
                .locale(globalConfig.locale) 
                .format('a hh:mm:ss')
              %></a></div>
              <div class="from">
                <a style="color: <%= getColor(message.from); %>" href="/message/<%= message._id %>">
                  <%= message.from %>
                </a>
              </div>
              <div class="word"><%- parseColor(escapeHTML(message.message))%></div>
            </div>
          <% } else { %>
            <div id="<%= message._id %>" class="message">
              <div class="time"><a href="#<%= message._id %>"><%= 
                moment(message.time)
                .utcOffset(globalConfig.timezone)
                .locale(globalConfig.locale) 
                .format('a hh:mm:ss')
              %></a></div>
              <div class="from">
                <a href="/message/<%= message._id %>">
                  *
                </a>
              </div>
              <div class="word"><a href="/message/<%= message._id %>" style="color: <%= getColor(message.from); %>" ><%= message.from %></a> <%- parseColor(escapeHTML((/^\u0001ACTION\s*(.+)\s*\u0001$/i).exec(message.message)[1]))%></div>
            </div>
          <% } %>
        <% }); %>
    </div>
    <div class="anchor" id="bottom"></div>
    <script>
      head.load(["//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.4/jquery.min.js"], function () {
        /**
         * Check a href for an anchor. If exists, and in document, scroll to it.
         * If href argument ommited, assumes context (this) is HTML Element,
         * which will be the case when invoked by jQuery after an event
         */
        function scroll_if_anchor(href) {
            href = typeof(href) == "string" ? href : $(this).attr("href");
            
            // You could easily calculate this dynamically if you prefer
            var fromTop = 50;
            
            // If our Href points to a valid, non-empty anchor, and is on the same page (e.g. #foo)
            // Legacy jQuery and IE7 may have issues: http://stackoverflow.com/q/1593174
            if(href.indexOf("#") == 0) {
                var $target = $(href);
                // Older browser without pushState might flicker here, as they momentarily
                // jump to the wrong position (IE < 10)
                if($target.get().length) {
                    $('html, body').animate({ scrollTop: $target.offset().top - fromTop });
                    if(history && "pushState" in history) {
                        history.pushState({}, document.title, window.location.pathname + href);
                        return false;
                    }
                }
            }
        }    
        
        // When our page loads, check to see if it contains and anchor
        scroll_if_anchor(window.location.hash);
        
        // Intercept all anchor clicks
        $("body").on("click", "a", scroll_if_anchor);
      });
    </script>
    
    <script>
      head.load([
        "//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.4/jquery.min.js",
        "/components/fancybox/lib/jquery.mousewheel-3.0.6.pack.js", 
        "/components/fancybox/source/jquery.fancybox.css?v=2.1.5",                                                                                                            
        "/components/fancybox/source/jquery.fancybox.pack.js?v=2.1.5",                                                                                                      
        "/components/fancybox/source/helpers/jquery.fancybox-buttons.css?v=1.0.5",                                                                                                      
        "/components/fancybox/source/helpers/jquery.fancybox-buttons.js?v=1.0.5",                                                                                                      
        "/components/fancybox/source/helpers/jquery.fancybox-media.js?v=1.0.6",                                                                                                 
        "/components/fancybox/source/helpers/jquery.fancybox-thumbs.css?v=1.0.7",                                                                                                 
        "/components/fancybox/source/helpers/jquery.fancybox-thumbs.js?v=1.0.7",
        "/js/jquery.lazyload.js",
        "/js/channel-common.js",
